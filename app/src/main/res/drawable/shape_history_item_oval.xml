    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/transparent" />
            <stroke android:width="0.5dp" android:color="#cccccc" android:dashWidth="5dp" android:dashGap="1.5dp" />
        </shape>
    </item>
</selector>